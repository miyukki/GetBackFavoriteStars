{
  "name": "Get Back Favorite Stars",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "We will get back twitter's favorite stars.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "http://twitter.com/",
    "https://twitter.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "css": [
        "styles/twitter.css"
      ],
      "js": [
        "scripts/twitter.js"
      ]
    }
  ]
}
